{% from 'components/nickname.jinja2' import clickable_nickname with context %}

{% for submission in submission_list %}
  <div class="list">
    <div class="item">
      {{ submission.id }} {{ clickable_nickname(submission.author) }} {{ submission.create_time | date('Y-m-d H:i:s') }}
    </div>
  </div>
{% endfor %}