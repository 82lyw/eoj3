{% extends 'contest/base.jinja2' %}
{% block contest_content %}


  <div class="content-header">
    <h2>Online status</h2>
  </div>

  <table class="table">
    <thead>
      <tr>
        <th width="9%">#</th>
        <th width="14%">Who</th>
        <th width="32%">Problem</th>
        <th width="25%">Verdict</th>
        <th width="20%">When</th>
      </tr>
    </thead>
    <tbody>
      {% for submission in submission_list %}
        <tr>
          <td scope="row">{{ submission.pk }}</td>
          <td>{{ submission.author }}</td>
          <td>{{ submission.problem }}</td>
          <td class="text-{{ submission.get_verdict_color() }}">{{ submission.get_status_display() }}</td>
          <td>{{ submission.create_time }}</td>
        </tr>
      {% endfor %}
    </tbody>
  </table>

  {{ my_paginator() }}
{% endblock %}